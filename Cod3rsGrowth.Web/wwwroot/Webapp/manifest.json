{
  "_version": "1.58.0",
  "sap.app": {
    "id": "ui5.carro",
    "i18n": {
      "bundleUrl": "i18n/i18n.properties",
      "supportedLocales": [
        ""
      ],
      "fallbackLocale": ""
    },
    "title": "",
    "description": "",
    "type": "application",
    "applicationVersion": {
      "version": "1.0.0"
    },
    "dataSources": {
      "invoiceRemote": {
        "uri": "V2/Northwind/Northwind.svc/",
        "type": "OData",
        "settings": {
          "odataVersion": "2.0"
        }
      }
    }
  },
  "sap.ui": {
    "technology": "UI5",
    "deviceTypes": {
      "desktop": true,
      "tablet": true,
      "phone": true
    }
  },
  "sap.ui5": {
    "contentDensities": {
      "compact": true,
      "cozy": true
    },
    "rootView": {
      "viewName": "ui5.carro.view.App",
      "type": "XML",
      "async": true,
      "id": "app"
    },
    "dependencies": {
      "minUI5Version": "1.108.0",
      "libs": {
        "sap.m": {},
        "sap.ui.core": {}
      }
    },
    "models": {
      "i18n": {
        "type": "sap.ui.model.resource.ResourceModel",
        "settings": {
          "bundleName": "ui5.carro.i18n.i18n",
          "supportedLocales": [
            ""
          ],
          "fallbackLocale": ""
        }
      }
    },
    "routing": {
      "config": {
        "routerClass": "sap.m.routing.Router",
        "type": "View",
        "viewType": "XML",
        "path": "ui5.carro.view",
        "controlId": "app",
        "controlAggregation": "pages",
        "transition": "slide",
        "bypassed": {
          "target": "notFound"
        },
        "async": true
      },
      "routes": [
        {
          "pattern": "",
          "name": "appListagem",
          "target": "listagem"
        },
        {
          "pattern": "AdicionarVenda",
          "name": "appAdicionarVenda",
          "target": "adicionarVenda"
        },
        {
          "pattern": "detalhes/{id}",
          "name": "appDetalhes",
          "target": "detalhes"
        }
      ],
      "targets": {
        "listagem": {
          "id": "listagem",
          "name": "ListagemVenda",
          "level": 1
        },
        "adicionarVenda": {
          "id": "adicionarVenda",
          "name": "AdicionarVenda"
        },
        "notFound": {
          "id": "notFound",
          "name": "NotFound",
          "transition": "show"
        },
        "detalhes": {
          "id": "detalhes",
          "name": "Detalhes"
        }
      }
    }
  }
}